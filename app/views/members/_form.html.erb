<div class="">

    <%=form_for @member, html: {class: ""} do |f| %>
        <% if @member.errors.any? %>
            <div id="errors_explanation">
                <h2> <%=pluralize(@member.errors.count, "error") %>
                prohibited this membr from being saved: </h2>
                
                <ul>
                    <% @member.errors.full_messages.each do |msg| %>
                        <li> <%=msg %></li>
                    <%end%>
                </ul>
            </div>
        <%end%>
    
    <fieldset>
        
			<div class="accountBlocks accountNewUser">
	    		<h4 class="blockhdng">Account Details</h4>
                <div class="inner formee">
		            <div class="clearfix">
		                <div class="reqFieldWrpr">
		                    <div class="formee-lbl right">
		                        <em class="formee-req">*</em>
		                        <span class="help">Indicates required field</span>
		                    </div>
				        </div>
                    </div>
                    
                    <div class="clearfix">
	                   <div class="one-half">
	                       <label>Email<em id="" class="formee-req">*</em></label>
						    <%=f.text_field :Email %>
                           <p class="formee-msg-note">username@hostname.com</p>
	                   </div>
				    </div>
                    
                    <div class="clearfix">
	                   <div class="one-fourth">
	                       <label>Password<em id="" class="formee-req">*</em></label>
						    <%=f.password_field :password %>
	                   </div>
	                   <div class="one-fourth">
	                       <label>Confirm Password<em id="" class="formee-req">*</em></label>
						    <%=f.password_field :password_confirmation %>
	                   </div>
				    </div>
                    
                </div>  
            </div>
			<div class="accountBlocks accountNewUser">
	    		<h4 class="blockhdng">Address</h4>
                <div class="inner formee">
                    <div class="clearfix">
                        <div class=" reqFieldWrpr">
                            <div class="formee-lbl right">
                                <em class="formee-req">*</em>
                                <span class="help">Indicates required field</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="clearfix">
	                   <div class="one-third">
	                       <label>First Name<em id="" class="formee-req">*</em></label>
						    <%=f.text_field :FirstName %>
	                   </div>
	                   <div class="one-third">
	                       <label>Last Name<em id="" class="formee-req">*</em></label>
						    <%=f.text_field :LastName %>
	                   </div>
				    </div>
                    
                    <div class="clearfix">
	                   <div class="two-third">
	                       <label>Address 1<em id="" class="formee-req">*</em></label>
						    <%=f.text_field :Address1 %>
	                   </div>
				    </div>
                    <div class="clearfix">
	                   <div class="two-third">
	                       <label>Address 2 </label>
						    <%=f.text_field :Address2 %>
	                   </div>
				    </div>
                    
                    <div class="clearfix">
	                   <div class="one-fourth">
	                       <label>City <em id="" class="formee-req">*</em></label>
						    <%=f.text_field :City %>
	                   </div>
                        <div class="one-fourth">
	                       <label>State <em id="" class="formee-req">*</em></label>
						    <%= f.collection_select :State, State.all, :state_abbr, :state_name %>
	                   </div>
                        <div class="one-fourth">
	                       <label>Zip <em id="" class="formee-req">*</em></label>
						    <%=f.text_field :Zip %>
	                   </div>
				    </div>
                    <div class="clearfix">
                      <div class="actions">
                        <%= f.submit "Register", :class => 'searchButton' %>
                      </div>
				    </div>
                    
                </div>
            </div>
            
                
        

    </fieldset>
    
    <%end%>
    
</div>